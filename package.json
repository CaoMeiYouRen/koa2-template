    "@types/koa-bodyparser": "^4.3.9",
    "@types/koa-cache-control": "^2.0.5",
    "@types/koa-log4": "^2.3.6",
    "@types/koa-static": "^4.0.4",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^8.0.11",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/module-alias": "^2.0.4",
