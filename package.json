    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cache-control": "^2.0.1",
    "@types/koa-log4": "^2.3.2",
    "@types/koa-static": "^4.0.1",
    "@types/koa__cors": "^3.0.2",
    "@types/koa__router": "^8.0.3",
    "@types/lodash": "^4.14.167",
    "@types/mocha": "^8.2.0",
    "@types/module-alias": "^2.0.0",
